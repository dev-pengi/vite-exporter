{
  "name": "vite-exporter",
  "version": "2.0.0",
  "type": "module",
  "description": "A vite plugin that auto-generate index files to make exports/imports more readable and centralized",
  "author": "Sifedine Haboul, hexxt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-pengi/vite-exporter"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p .",
    "prepare": "yarn build",
    "changeset": "changeset"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "devDependencies": {
    "@changesets/cli": "^2.18.0",
    "@types/node": "^22.0.0",
    "changeset": "^0.2.6",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "minimatch": "^10.0.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.4"
  }
}
